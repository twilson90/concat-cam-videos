    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "console": "integratedTerminal", 
            "outputCapture": "std",
            "cwd": "C:/Videos/",
            "program": "${workspaceFolder}\\index.js",
            "args": [
                "--timestamps=hard",
                "--vcodec=hevc_nvenc",
                "--vcodec_opts=profile:v=main,rc:v=constqp,qp=24",
                "--upload"
            ]
        }
    ]
}